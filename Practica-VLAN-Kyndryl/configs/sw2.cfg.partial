#sw2
!
vlan 10
    name VLAN10
vlan 20
    name VLAN20
vlan 30
    name VLAN30
!
interface e0/1
    switchport trunk encapsulation dot1q
    switchport mode trunk
    switchport trunk allowed vlan 10,20,30
    no shutdown
!
interface e0/2
    switchport trunk encapsulation dot1q
    switchport mode trunk
    switchport trunk allowed vlan 10,20,30
    no shutdown
!
